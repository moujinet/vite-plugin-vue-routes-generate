{
  "name": "[name]",
  "version": "0.0.0",
  "description": "vite-plugin-[name]",
  "author": "Allen Luo <luoyi@mouji.net>",
  "license": "MIT",
  "homepage": "https://github.com/moujinet/[name]#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moujinet/[name].git"
  },
  "bugs": {
    "url": "https://github.com/moujinet/[name]/issues"
  },
  "keywords": [
    "vite",
    "plugin",
    "vite-plugin"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm",
    "dev": "tsup src/index.ts --dts --watch",
    "play": "pnpm run --filter ./playground dev",
    "prepublishOnly": "pnpm run build",
    "test": "vitest",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.0.2",
    "vite": "^4.2.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.4",
    "@types/node": "^18.15.11",
    "bumpp": "^9.1.0",
    "eslint": "^8.38.0",
    "lint-staged": "^13.2.1",
    "pnpm": "^8.2.0",
    "simple-git-hooks": "^2.8.1",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5",
    "vitest": "^0.30.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
