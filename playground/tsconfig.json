{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ES2017",
    "module": "ESNext",
    "lib": ["DOM", "ESNext"],
    "strict": true,
    "esModuleInterop": true,
    "incremental": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "types": [
      "vite/client",
      "vite-plugin-vue-routes-generate/client"
    ],
    "paths": {
      "vite": ["node_modules/vite"]
    }
  },
  "include": ["src"],
  "exclude": ["dist", "node_modules"]
}
